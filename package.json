    "karma-ui5": "^2.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.5",
    "prettier": "^2.1.2",
    "@sapui5/ts-types": "1.78.10",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0"
  }
